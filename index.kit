		<script type="text/javascript">

		// Inject YouTube API script
		var tag = document.createElement('script');
		tag.src = "//www.youtube.com/player_api";
		var firstScriptTag = document.getElementsByTagName('script')[0];
		firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

		function onYouTubePlayerAPIReady() {

		}

		$(document).ready(function() {

			function videoclick() {
				$('.video-click').on('click', function(e) {
					$(this).off('click').removeAttr('href').removeClass('video-click').addClass('video-click-active');
					vid = $(this).data("video-id");
					e.preventDefault();
					$('#playbutton').fadeOut(500, function() {
						$("#the-video").html('<iframe src="//www.youtube.com/embed/' + vid + '?wmode=transparent&showinfo=0&controls=0&rel=0&autoplay=1&enablejsapi=1" frameborder="0" allowfullscreen></iframe>');
					});
					$("#the-video").fadeIn(1000);
					$(".closebutton").fadeIn(1000);
				});
			}

			videoclick();

			$('.closebutton').on('click', function() {
				$(this).hide( 1, function() {
					videoclick();
				});
				$('#the-video').html('').hide();
				$('#playbutton').show();
				$('.video-click-active').removeClass('video-click-active').addClass('video-click');
			});
		});
		</script>

